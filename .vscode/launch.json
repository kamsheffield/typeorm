{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Test Integration",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "--file",
                "./build/compiled/test/utils/test-setup.js",
                "--bail",
                "--recursive",
                "--timeout",
                "90000",
                "./build/compiled/test/integration/sample1-simple-entity.js",
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/build/compiled/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "--file",
                "./build/compiled/test/utils/test-setup.js",
                "--bail",
                "--recursive",
                "--timeout",
                "90000",
                "./build/compiled/${relativeFileDirname}/${fileBasenameNoExtension}.js",
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/build/compiled/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test Selected",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "--file",
                "./build/compiled/test/utils/test-setup.js",
                "--bail",
                "--recursive",
                "--timeout",
                "90000",
                "./build/compiled/test",
                "--grep=${selectedText}"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/build/compiled/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test All",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "--file",
                "./build/compiled/test/utils/test-setup.js",
                "--bail",
                "--recursive",
                "--timeout",
                "90000",
                "./build/compiled/test",
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/build/compiled/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch PS Migration",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "./build/compiled/src/cli-ts-node-esm.js",
            "args": [
                "schema:log",
                "-d",
                "./test/other-issues/planetscale-migrations/planetscale-datasource.ts",
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/build/compiled/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch MySql Migration",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "./build/compiled/src/cli-ts-node-esm.js",
            "args": [
                "schema:log",
                "-d",
                "./test/other-issues/planetscale-migrations/mysql-datasource.ts",
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/build/compiled/**/*.js"
            ]
        }
    ]
}
